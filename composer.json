{
    "name": "topphp/topphp-rsa",
    "type": "library",
    "description": "RSA 非对称加密解密",
    "license": "MIT",
    "authors": [
        {
            "name": "topphp",
            "email": "sleep@kaituocn.com",
            "homepage": "https://github.com/topphp",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~7.2",
        "ext-openssl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "7.5.20",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Topphp\\TopphpRsa\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Topphp\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "think": {
            "services": [],
            "config": []
        }
    },
    "config": {
        "sort-packages": true
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://mirrors.aliyun.com/composer"
        }
    }
}
